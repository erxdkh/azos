<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Azos.Web</id>
    <version>$version$</version>
    <authors>Azist, @dzhabis, @dkh</authors>
    <owners>Azist, @dzhabis, @dkh</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>http://zhabis.com/</projectUrl>
    <iconUrl>$icon$</iconUrl>
    <description>AZOS - A to Z Application OS. Web Assembly</description>
    <releaseNotes>$headline$</releaseNotes>
    <copyright>2019 Azist Group</copyright>
    <tags>atoz fullstack full-stack apps net-core big-memory pile slim glue wave cluster zhabis</tags>
    <repository type="GIT" url="https://github.com/azist/azos" />
    
    <dependencies>
      <dependency id="Azos" version="$version$" />
    </dependencies>
    
    <contentFiles>
      <files include="**/*" buildAction="None" copyToOutput="true" flatten="false" />
    </contentFiles>
    
  </metadata>

  <files>
  
    <!-- Core tools -->
  
    <file src="..\..\out\Release\Azos.Web.dll"   target="contentFiles\any\any\azos\run-core" />
    <file src="..\..\out\Release\Azos.Web.xml"   target="contentFiles\any\any\azos\run-core" />
    <file src="..\..\out\Release\Azos.Web.pdb"   target="contentFiles\any\any\azos\run-core" />
    
    <!-- Netf tools -->
    
    <file src="..\..\out\Release\Azos.Web.dll"   target="contentFiles\any\any\azos\run-netf" />
    <file src="..\..\out\Release\Azos.Web.xml"   target="contentFiles\any\any\azos\run-netf" />
    <file src="..\..\out\Release\Azos.Web.pdb"   target="contentFiles\any\any\azos\run-netf" />
    
    <!-- ============================================================ -->
  
    <file src="..\..\out\Release\Azos.Web.dll" target="lib\netstandard2.0" />
    <file src="..\..\out\Release\Azos.Web.xml" target="lib\netstandard2.0" />
    <file src="..\..\out\Release\Azos.Web.pdb" target="lib\netstandard2.0" />
   
    <file src="..\..\out\Release\Azos.Web.dll"   target="lib\netcoreapp2.0" />
    <file src="..\..\out\Release\Azos.Web.xml"   target="lib\netcoreapp2.0" />
    <file src="..\..\out\Release\Azos.Web.pdb"   target="lib\netcoreapp2.0" />
    
    <file src="..\..\out\Release\Azos.Web.dll"   target="lib\net472" />
    <file src="..\..\out\Release\Azos.Web.xml"   target="lib\net472" />
    <file src="..\..\out\Release\Azos.Web.pdb"   target="lib\net472" />
    
  </files>

</package>