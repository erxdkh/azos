{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "COMMENT-----XXXsrc/Azos.Web/Azos.Web.csproj",
            "src/**/Azos*.csproj"
          ],
          "exclude": [
            "src/**/Azos.Tests.**.csproj"
          ]
        }
      ],
      "dest": "out/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "doc/**/*.md",
          "src/**/*.md",
          "out/api/**/*.yml",
          "out/api/toc.yml",
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "doc/img/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "src/**"
        ],
        "exclude": [
          "obj/**",
          "out/dfx-site/**"
        ]
      }
    ],
    "dest": "out/dfx-site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
      "elm/docfx/template-override"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,

    "globalMetadata": {
      "_appTitle": "A to Z Sky OS",
      "_appFooter": "Copyright (c) 2020 Azist Group",
      "_appLogoPath": "doc/img/azos-toolbar-emblem.96.png",
      "_appFaviconPath": "doc/img/favicon.ico",
      "_disableBreadcrumb": true,
      "_disableAffix": false,
      "_disableContribution": true
    }
  }
}
